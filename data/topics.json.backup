        "entryCount": 25,
        "entries": [
            {"id": "olum1", "content": "hayatın kaçınılmaz sonu. herkesin bir gün karşılaşacağı gerçek.", "author": "filozof", "date": "20.12.2025", "time": "01:50", "isSpecial": false},
            {"id": "olum2", "content": "bazıları için son, bazıları için yeni bir başlangıç olarak görülür.", "author": "dusunur", "date": "20.12.2025", "time": "01:51", "isSpecial": false},
            {"id": "olum3", "content": "ölüm korkusu insanın en büyük korkularından biridir.", "author": "psikolog", "date": "20.12.2025", "time": "01:52", "isSpecial": false},
            {"id": "olum4", "content": "memento mori - ölümü hatırla. antik roma'dan gelen bir öğüt.", "author": "tarihci", "date": "20.12.2025", "time": "01:53", "isSpecial": false},
            {"id": "olum5", "content": "hayatın değerini anlamak için ölümü düşünmek gerekir.", "author": "bilge", "date": "20.12.2025", "time": "01:54", "isSpecial": false},
            {"id": "olum6", "content": "her canlının kaderi. doğumla başlayan yolculuğun sonu.", "author": "naturalist", "date": "20.12.2025", "time": "01:55", "isSpecial": false},
            {"id": "olum7", "content": "ölüm sonrası hayat inancı birçok dinde merkezi bir konudur.", "author": "teolog", "date": "20.12.2025", "time": "01:56", "isSpecial": false},
            {"id": "olum8", "content": "yaşamın anlamını sorgularken ölümü de sorgulamak gerekir.", "author": "egzistansiyalist", "date": "20.12.2025", "time": "01:57", "isSpecial": false},
            {"id": "olum9", "content": "ölüm sanatın en çok işlenen temalarından biridir.", "author": "sanatci", "date": "20.12.2025", "time": "01:58", "isSpecial": false},
            {"id": "olum10", "content": "her ölüm bir kayıptır ama hayat devam eder.", "author": "realist", "date": "20.12.2025", "time": "01:59", "isSpecial": false},
            {"id": "olum11", "content": "ölüm korkusu yerine yaşam sevgisi taşımak daha anlamlı.", "author": "optimist", "date": "20.12.2025", "time": "02:00", "isSpecial": false},
            {"id": "olum12", "content": "ölüm hakkında konuşmak tabu olmaktan çıkmalı.", "author": "sosyolog", "date": "20.12.2025", "time": "02:01", "isSpecial": false},
            {"id": "olum13", "content": "her ölüm bir hikayenin sonu, bir mirasın başlangıcıdır.", "author": "hikayeci", "date": "20.12.2025", "time": "02:02", "isSpecial": false},
            {"id": "olum14", "content": "ölüm karşısında herkes eşittir.", "author": "adalet", "date": "20.12.2025", "time": "02:03", "isSpecial": false},
            {"id": "olum15", "content": "yaşamın kıymetini bilmek için ölümü anlamak şart.", "author": "mudrik", "date": "20.12.2025", "time": "02:04", "isSpecial": false},
            {"id": "olum16", "content": "ölüm bir son değil, bir dönüşümdür.", "author": "mistik", "date": "20.12.2025", "time": "02:05", "isSpecial": false},
            {"id": "olum17", "content": "her gün biraz daha ölüme yaklaşırız ama bu korkulacak bir şey değil.", "author": "stoik", "date": "20.12.2025", "time": "02:06", "isSpecial": false},
            {"id": "olum18", "content": "ölüm bizi sevdiklerimizle buluşturacak mı yoksa sonsuza kadar ayıracak mı?", "author": "sorgulayan", "date": "20.12.2025", "time": "02:07", "isSpecial": false},
            {"id": "olum19", "content": "ölüm anında neler hissedilir? kimse tam olarak bilemez.", "author": "merakli", "date": "20.12.2025", "time": "02:08", "isSpecial": false},
            {"id": "olum20", "content": "ölüm korkusu yaşam korkusundan daha az değildir.", "author": "cesur", "date": "20.12.2025", "time": "02:09", "isSpecial": false},
            {"id": "olum21", "content": "her ölüm bir ders, her yaşam bir fırsat.", "author": "ogretmen", "date": "20.12.2025", "time": "02:10", "isSpecial": false},
            {"id": "olum22", "content": "ölüm sonrası ne olacağını kimse kesin olarak bilemez.", "author": "agnostik", "date": "20.12.2025", "time": "02:11", "isSpecial": false},
            {"id": "olum23", "content": "yaşamın anlamı ölümle tamamlanır.", "author": "tamamlayan", "date": "20.12.2025", "time": "02:12", "isSpecial": false},
            {"id": "olum24", "content": "ölüm bir kapı, ardında ne olduğunu ancak geçenler bilir.", "author": "gizem", "date": "20.12.2025", "time": "02:13", "isSpecial": false},
            {"id": "olum25", "content": "her şeyin sonu var, ölüm de bunun bir parçası.", "author": "kabullenen", "date": "20.12.2025", "time": "02:14", "isSpecial": false}
